                if (access.multiSession) {
                  this.isMultiSession = true;
                  sessionStorage.setItem(access.sessionName, JSON.stringify(session));
                } else {
                  this.isMultiSession = false;
                  localStorage.setItem(access.sessionName, JSON.stringify(session));
                }
              } else {
